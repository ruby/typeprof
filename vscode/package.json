{
    "name": "typeprof",
    "displayName": "Ruby TypeProf",
    "version": "0.20.0",
    "publisher": "mame",
    "author": {
        "name": "Yusuke Endoh"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mame/vscode-typeprof"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "Ruby",
        "language server"
    ],
    "engines": {
        "vscode": "^1.54.0"
    },
    "extensionKind": [
        "workspace"
    ],
    "activationEvents": [
        "onLanguage:ruby"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/node": "^14.14.37",
        "typescript": "^4.2.3",
        "vsce": "^1.96.1",
        "vscode": "^1.1.37"
    },
    "dependencies": {
        "vscode-languageclient": "^7.0.0"
    }
}
